#!/usr/bin/env python

import sys
import os

import common

from osbuild import config

common.setup()

karma_path = os.path.join(config.bin_dir, "karma")
karma_args = " ".join(sys.argv[1:])
os.environ["SUGAR_RUN_TEST"] = "%s %s" % (karma_path, karma_args)

command = ["sugar-runner"]
os.execvp(command[0], command)
